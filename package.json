{
  "name": "snakecase-keys",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "version": "8.0.1",
  "description": "Convert an object's keys to snake case",
  "license": "MIT",
  "repository": "bendrucker/snakecase-keys",
  "author": {
    "name": "Ben Drucker",
    "email": "bvdrucker@gmail.com",
    "url": "bendrucker.me"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "test": "vitest --typecheck",
    "attw": "attw --pack . --ignore-rules false-export-default"
  },
  "keywords": [
    "snake",
    "case",
    "camel",
    "keys",
    "object"
  ],
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "tsdown": "^0.12.9",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "change-case": "^4.1.2",
    "map-obj": "^4.2.0",
    "type-fest": "^4.15.0"
  },
  "engines": {
    "node": ">=20"
  }
}

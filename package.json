{
  "name": "snakecase-keys",
  "version": "8.0.1",
  "description": "Convert an object's keys to snake case",
  "license": "MIT",
  "repository": "bendrucker/snakecase-keys",
  "author": {
    "name": "Ben Drucker",
    "email": "bvdrucker@gmail.com",
    "url": "https://bendrucker.me"
  },
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "test": "prettier --check src/ && npm run build && vitest run",
    "format": "prettier --write src/",
    "build": "rm -rf dist && tsc && tsc -p tsconfig.cjs.json",
    "prepare": "npm run build"
  },
  "files": [
    "index.d.ts",
    "dist"
  ],
  "keywords": [
    "snake",
    "case",
    "snakecase",
    "key",
    "keys",
    "object",
    "map"
  ],
  "dependencies": {
    "change-case": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "prettier": "^3.1.1",
    "tsd": "^0.30.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "es5"
  }
}
